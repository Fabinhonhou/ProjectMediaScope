modules = ["web", "nodejs-20", "bash", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["cacert", "cargo", "freetype", "grpc", "lcms2", "libffi", "libiconv", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "openssl", "pgadmin4", "pkg-config", "postgresql", "rustc", "tcl", "tk", "zlib"]

[userenv]

[userenv.development]
DEBUG = "True"
SECRET_KEY = "dev-secret-key-change-in-production-abc123xyz789"

[workflows]
